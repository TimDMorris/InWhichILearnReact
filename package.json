{
  "name": "in-which-ilearn-react",
  "packageManager": "npm@6.x",
  "version": "1.9.1",
  "description": "It's a repository in which I'm learning React, it's shared in case it's useful for anyone else.",
  "license": "MIT",
  "homepage": "https://timdmorris.github.io/InWhichILearnReact/",
  "scripts": {
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:stylelint": "x affected --target=stylelint",
    "affected:test": "nx affected:test",
    "affected": "nx affected",
    "build": "nx build",
    "build:portfolio": "nx build --project=portfolio --production",
    "cherry-pick-all": "./tools/cherry-pick-all.sh",
    "dep-graph": "nx dep-graph",
    "e2e": "nx e2e",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "format": "nx format:write",
    "help": "nx help",
    "lint": "nx workspace-lint && nx lint",
    "migrate-all": "./tools/migrate-all.sh",
    "nx": "nx",
    "start": "nx serve",
    "test": "nx test",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator"
  },
  "private": true,
  "dependencies": {
    "core-js": "^3.6.5",
    "nx": "nx",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nrwl/cli": "14.5.7",
    "@nrwl/cypress": "14.5.7",
    "@nrwl/eslint-plugin-nx": "^14.5.7",
    "@nrwl/jest": "14.5.7",
    "@nrwl/linter": "14.5.7",
    "@nrwl/react": "14.5.7",
    "@nrwl/web": "14.5.7",
    "@nrwl/workspace": "14.5.7",
    "@qiwi/semantic-release-gh-pages-plugin": "^5.2.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/react": "13.3.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "29.2.2",
    "cypress": "^10.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "27.5.1",
    "nx": "14.5.7",
    "nx-stylelint": "^13.5.2",
    "prettier": "^2.6.2",
    "react-axe": "^3.5.4",
    "react-test-renderer": "18.2.0",
    "semantic-release": "^19.0.3",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "ts-jest": "27.1.4",
    "ts-node": "10.9.1",
    "typescript": "~4.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimDMorris/InWhichILearnReact.git"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github",
      [
        "@qiwi/semantic-release-gh-pages-plugin",
        {
          "src": "dist/apps/portfolio"
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
